<div class = "well span3">
	<p>
		<% if(@title && @price && @image_url)  %>
		  <ul class="nav nav-list">
		    <% @title.zip(@price, @image_url).each do |t, p, i| %>
		      <li><%= t %></li>
		      <li><%= i %></li>
		      <li><%= p %></li>
		      <hr/>
		    <% end %>
		  </ul>
		<% end %>
	</p>
</div>
